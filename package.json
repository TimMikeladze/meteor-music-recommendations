{
  "name": "music-recommendations",
  "private": "true",
  "version": "1.0.0",
  "description": "An app and guide showcasing how to build a music recommendation site using Apollo",
  "main": "index.js",
  "scripts": {
    "start": "meteor run",
    "pretest": "npm run lint --silent",
    "test": "meteor test --once --driver-package dispatch:mocha-phantomjs",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimMikeladze/music-recommendations.git"
  },
  "author": "Tim Mikeladze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimMikeladze/music-recommendations/issues"
  },
  "homepage": "https://github.com/TimMikeladze/music-recommendations#readme",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "airbnb"
